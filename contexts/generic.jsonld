{
    "@context": {
        "@version": 1.1,
        "pav": "http://purl.org/pav/",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "repronim": "https://schema.repronim.org/",
        "schema": "http://schema.org/",
        "schema:description": {
            "@type": "xsd:string",
            "@container": "@language"
        },
        "schema:schemaVersion": {
            "@type": "xsd:string"
        },
        "schema:version": {
            "@type": "xsd:string"
        },
        "responseOptions": {
            "@id": "repronim:valueconstraints",
            "@type": "@id"
        },
        "@language": "en",
        "skos:prefLabel": { "@id": "http://www.w3.org/2004/02/skos/core#prefLabel", "@container": "@language" },
        "skos:altLabel": { "@id": "http://www.w3.org/2004/02/skos/core#altLabel", "@container": "@language" },
        "preamble": { "@id": "http://schema.org/question", "@container": "@language" },
        "question": {
            "@id": "http://schema.org/question",
            "@type": "xsd:string",
            "@container": "@language"
        },
        "ui" : "@nest",
        "ui_order": {
            "@id": "https://schema.repronim.org/ui_order",
            "@container": "@list",
            "@type": "@vocab",
            "@nest": "ui"
        },
        "inputType": {
            "@id": "https://schema.repronim.org/inputType",
            "@type": "xsd:string",
            "@nest": "ui"
        },
        "ui_shuffle": {
            "@id": "https://schema.repronim.org/shuffle",
            "@type": "schema:Boolean",
            "@nest": "ui"
        },
        "branchLogic": {
            "@id": "https://schema.repronim.org/branchLogic",
            "@container": "@index"
        },
        "scoringLogic": {
            "@id": "https://schema.repronim.org/scoringLogic",
            "@container": "@index"
        },
        "skipped": "https://schema.repronim.org/refused_to_answer",
        "dontKnow": "https://schema.repronim.org/dont_know_answer",
        "timedOut": "https://schema.repronim.org/timed_out",
        "pav:importedFrom": {
            "@id": "http://purl.org/pav/importedFrom",
            "@type": "@id"
        },
        "pav:importedBy": {
            "@id": "http://purl.org/pav/importedBy",
            "@type": "@id"
        },
        "pav:createdWith": {
            "@id": "http://purl.org/pav/createdWith",
            "@type": "@id"
        },
        "pav:createdBy": {
            "@id": "http://purl.org/pav/createdBy",
            "@type": "@id"
        },
        "pav:createdOn": {
            "@id": "http://purl.org/pav/createdOn",
            "@type": "@id"
        },
        "pav:previousVersion": {
            "@id": "http://purl.org/pav/previousVersion",
            "@type": "@id"
        },
        "pav:lastUpdateOn": {
            "@id": "http://purl.org/pav/lastUpdateOn",
            "@type": "@id"
        },
        "pav:derivedFrom": {
            "@id": "http://purl.org/pav/derivedFrom",
            "@type": "@id"
        }
    }
}
