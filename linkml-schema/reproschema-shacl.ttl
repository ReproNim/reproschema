    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Additional properties" ;
    sh:property [ sh:description "['A pointer to the node describing the item.']" ;
            sh:maxCount 1 ;
            sh:name "isAbout" ;
            sh:or ( [ sh:node reproschema:Activity ] [ sh:node reproschema:Field ] ) ;
