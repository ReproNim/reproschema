            sh:order 2 ;
            sh:path reproschema:overrideProperties ],
        [ sh:node reproschema:AdditionalProperty ;
            sh:description "['An array of objects to describe the various properties added to assessments and fields.']" ;
            sh:name "addProperties" ;
            
            sh:order 1 ;
            sh:path reproschema:addProperties ],
        [ sh:datatype xsd:boolean ;
